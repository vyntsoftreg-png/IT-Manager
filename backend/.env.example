PORT=3001
NODE_ENV=development
# JWT Configuration
JWT_SECRET=your_jwt_secret_key_change_in_production
JWT_EXPIRES_IN=8h

# Security Keys
# Must be exactly 32 chars for AES-256
ENCRYPTION_KEY=itmanager-secret-key-32-chars!!
CORS_ORIGIN=http://localhost:5173

# Admin Seed (Optional)
ADMIN_PASSWORD=admin123
ADMIN_EMAIL=admin@itmanager.local

# Database (if using one that needs config)
DB_DIALECT=sqlite
DB_STORAGE=./database.sqlite
